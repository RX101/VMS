<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawerLayoutSignOut"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.a15041867.vms.SignOutActivity"
    tools:showIn="@layout/activity_sign_out">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/lvCurrentVisitor"></ListView>

    </LinearLayout>

    <android.support.design.widget.NavigationView
        android:id="@+id/nvSignOut"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:menu="@menu/navigation_menu_security">

    </android.support.design.widget.NavigationView>



</android.support.v4.widget.DrawerLayout>
